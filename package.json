{
  "name": "products-authenticity",
  "version": "0.0.1",
  "scripts": {
    "start": "node ./dist/server/index.js",
    "build:android": "tns build android",
    "build:ios": "tns build ios",
    "build:mobile": "tns build",
    "run:android": "tns run android --bundle",
    "run:ios": "tns run ios --bundle",
    "run:mobile": "tns run --bundle",
    "preview": "tns preview --bundle",
    "start-client": "ng serve --open --host products-auth.local.net --proxy-config ./client/proxy.conf.json",
    "start-server": "nodemon  --config ./server/nodemon.json --exec \"tsc -p server/tsconfig.json && node ./dist/server/index.js\"",
    "build-client": "rimraf ./dist/products-authenticity && ng build --prod",
    "build-server": "rimraf ./dist/server && tsc -p server/tsconfig.json",
    "build": "npm run build-client && npm run build-server",
    "test": "ng test",
    "lint-client": "ng lint",
    "lint-server": "tslint -c ./tslint.json -p ./server",
    "lint-client:fix": "ng lint --fix",
    "lint-server:fix": "tslint -c ./tslint.json -p ./server --fix",
    "e2e": "ng e2e"
  },
  "private": true,
  "engines": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "dependencies": {
    "@angular-redux/form": "^10.0.0",
    "@angular-redux/router": "^10.0.0",
    "@angular-redux/store": "^10.0.0",
    "@angular/animations": "^8.1.2",
    "@angular/cdk": "^8.1.1",
    "@angular/cli": "^8.1.2",
    "@angular/common": "^8.1.2",
    "@angular/compiler": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/core": "^8.1.2",
    "@angular/forms": "^8.1.2",
    "@angular/http": "^8.0.0-beta.10",
    "@angular/material": "^8.1.1",
    "@angular/platform-browser": "^8.1.2",
    "@angular/platform-browser-dynamic": "^8.1.2",
    "@angular/router": "^8.1.2",
    "@tsed/common": "^5.25.1",
    "@tsed/core": "^5.25.1",
    "@tsed/di": "^5.25.1",
    "@tsed/typeorm": "^5.25.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.0.0",
    "ethereumjs-tx": "^2.1.1",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "hammerjs": "^2.0.8",
    "highcharts": "^7.2.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql": "^2.17.1",
    "nativescript-angular": "^8.1.0",
    "nativescript-barcodescanner": "^3.2.2",
    "nativescript-theme-core": "^1.0.6",
    "nativescript-ui-sidedrawer": "^7.0.0",
    "qr-scanner": "^1.1.1",
    "qrcode": "^1.4.1",
    "redux": "4.0.1",
    "redux-observable": "^1.1.0",
    "redux-observable-es6-compat": "^1.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.4.0",
    "tns-core-modules": "^6.0.1",
    "ts-httpexceptions": "^4.1.0",
    "tslib": "^1.9.0",
    "typescript": "3.4.5",
    "uuid": "^3.3.3",
    "web3": "^1.2.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.801.2",
    "@angular/language-service": "^8.1.2",
    "@nativescript/schematics": "^0.6.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.3.14",
    "@types/jasminewd2": "^2.0.6",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.14.13",
    "codelyzer": "^5.0.0",
    "husky": "^3.0.3",
    "jasmine-core": "^3.4.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^6.3.14",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^2.0.6",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "nativescript-dev-webpack": "^1.0.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "protractor": "^5.4.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "@angular/compiler-cli": "^8.1.2"
  },
  "nativescript": {
    "id": "org.nativescript.ProductsAuthenticity",
    "tns-android": {
      "version": "6.0.0"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-client && npm run lint-server"
    }
  }
}
